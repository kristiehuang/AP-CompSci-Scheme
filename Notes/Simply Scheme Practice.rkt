;7.1
(define (gertrude wd)
  (let (asdf (if (vowel? (first wd)) 'an 'a))
  se asdf
      wd
      'is
      asdf
      wd
      'is
      asdf
      wd
    )
  )

(gertrude 'rose)
;(A ROSE IS A ROSE IS A ROSE)



